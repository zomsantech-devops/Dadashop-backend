{
	"version": 2,
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "(.*)",
			"dest": "index.js"
		}
	]
	,
	"crons" : [
		{
			"path" : "/api/v1/item/reset",
			"schedule" : "0 2 * * *"
		},
		{
			"path" : "/api/v1/item/reset",
			"schedule" : "0 14 * * *"
		}
	]
}